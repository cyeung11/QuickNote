<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item>

    <shape
        android:shape="oval">

        <solid
            android:color="#89bda7"/>

        <size
            android:width="48dp"
            android:height="48dp"/>


    </shape>

</item>

<item android:top="5sp"
    android:left="10sp">
    <rotate
        android:fromDegrees="-40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="-90">
        <shape android:shape="line">
            <stroke android:width="5dp"
                android:color="@android:color/white" />
        </shape>
    </rotate>
</item>


<item android:top="22sp"
    android:right="25sp"
    android:bottom="15sp">
    <rotate
        android:fromDegrees="50"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="-90">
        <shape android:shape="line">
            <stroke android:width="5dp" android:color="@android:color/white" />
        </shape>
    </rotate>
</item>


</layer-list>